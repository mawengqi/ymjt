<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ymjt.entity.Menu" table="menu">
        <id name="id" column="id" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property name="name" column="name" type="java.lang.String" not-null="true" length="20"/>
        <property name="modelId" column="modelid" type="java.lang.String" not-null="true"/>
        <property name="imageUrl" column="imageurl" type="java.lang.String" not-null="true"/>
    </class>
</hibernate-mapping>