            <generator class="identity"/>
        </id>
        <property name="name" column="name" type="java.lang.String" not-null="true" length="20"/>
        <property name="modelId" column="modelid" type="java.lang.Integer" not-null="true"/>
    </class>
</hibernate-mapping>